    "angular-local-storage": "^0.2.2",
    "angular-material": "^0.10.1",
    "angular-sanitize": "^1.4.4",
    "angular-translate": "^2.7.2",
    "angular-translate-storage-cookie": "^2.7.2",
    "angular-translate-storage-local": "^2.7.2",
    "angular-ui-router": "^0.2.15",
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "http-proxy": "^1.11.1"
  },
