{
  "name": "frontend",
  "version": "0.0.2",
  "description": "webpack angular dashboard",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack/webpack.build.js --bail -p",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --history-api-fallback --hot --inline --progress",
    "test": "karma start test/karma.conf.js",
    "test:dev": "jscs ./ && karma start --auto-watch --no-single-run",
    "metrics": "plato -r -d build/metrics src -t 'Frontend metrics'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Drathal/angular-webpack-dashboard.git"
  },
  "license": "MIT",
